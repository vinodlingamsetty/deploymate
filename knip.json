{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": [
    ".next/**",
    "coverage/**",
    "data/**",
    "dist/**",
    "docs-site/**",
    "public/**",
    "src/generated/**"
  ],
  "ignoreDependencies": [
    "@types/node",
    "@types/react",
    "@types/react-dom",
    "autoprefixer",
    "dotenv",
    "eslint-config-next",
    "postcss",
    "tailwindcss",
    "tailwindcss-animate"
  ],
  "entry": [
    "next.config.js",
    "prisma/seed.ts",
    "src/worker.ts"
  ],
  "project": [
    "src/**/*.{ts,tsx}",
    "prisma/**/*.ts",
    "*.config.{js,ts}",
    "next-env.d.ts"
  ]
}

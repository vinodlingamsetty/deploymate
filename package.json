{
  "name": "deploymate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "worker": "tsx src/worker.ts",
    "worker:dev": "tsx watch src/worker.ts",
    "worker:build": "esbuild src/worker.ts --bundle --platform=node --outfile=dist/worker.js --external:@prisma/client --external:@node-rs/argon2",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:prod": "docker compose up -d",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:integration:watch": "vitest --config vitest.config.integration.ts",
    "db:seed": "tsx prisma/seed.ts",
    "quality:unused": "npx -y knip --no-progress",
    "quality:dup": "npx -y jscpd --config .jscpd.json",
    "quality:deps": "npx -y dependency-cruiser --config .dependency-cruiser.cjs src --output-type err-long",
    "quality:semgrep": "mkdir -p .reports && npx -y @semgrep/semgrep scan --config auto --severity ERROR --error --json --output .reports/semgrep.json --exclude .next --exclude node_modules --exclude docs-site/build --exclude docs-site/node_modules",
    "quality:coverage": "npx -y c8 --check-coverage --lines 70 --functions 70 --statements 70 --branches 60 pnpm test",
    "quality:all": "pnpm quality:unused && pnpm quality:dup && pnpm quality:deps && pnpm quality:semgrep && pnpm quality:coverage",
    "docs:dev": "cd docs-site && npm run start",
    "docs:build": "cd docs-site && npm run build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@aws-sdk/client-s3": "^3.989.0",
    "@aws-sdk/s3-request-presigner": "^3.989.0",
    "@azure/storage-blob": "^12.31.0",
    "@google-cloud/storage": "^7.19.0",
    "@hookform/resolvers": "^5.2.2",
    "@node-rs/argon2": "^2.0.2",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.3.0",
    "adm-zip": "^0.5.16",
    "bplist-parser": "^0.3.2",
    "bullmq": "^5.69.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "^5.9.3",
    "lucide-react": "^0.563.0",
    "next": "^14.2.18",
    "next-auth": "5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "nodemailer": "^8.0.1",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "plist": "^3.1.0",
    "radix-ui": "^1.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^7.0.10",
    "@types/pg": "^8.16.0",
    "@types/plist": "^3.0.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.24",
    "dotenv": "^17.2.4",
    "esbuild": "^0.27.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "pino-pretty": "^13.1.3",
    "postcss": "^8.5.6",
    "prisma": "^7.3.0",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  }
}
